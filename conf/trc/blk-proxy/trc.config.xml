        <test name="driver_rebind" type="script">
            <objective/>
            <iter result="PASSED">
                <arg name="vendor_id"/>
                <arg name="device_id"/>
                <arg name="rebind_count"/>
            </iter>
        </test>

        <test name="restart_blk_proxy" type="script">
            <objective/>
