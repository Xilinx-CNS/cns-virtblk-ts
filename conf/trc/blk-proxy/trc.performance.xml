<test name="performance" type="package">
    <objective/>
    <iter result="PASSED">
        <test name="prologue" type="script">
            <objective/>
            <iter result="PASSED"/>
        </test>

        <test name="epilogue" type="script">
            <objective/>
            <iter result="PASSED"/>
        </test>

        <test name="fio_prologue" type="script" auxiliary="true">
            <objective/>
            <notes/>
            <iter result="PASSED">
                <arg name="bp-Ceph-Ceph"/>
                <arg name="bp-Nvme-TransportID"/>
                <arg name="bp-Malloc-NumberOfLuns"/>
                <arg name="bp-Malloc-LunSizeInMB"/>
                <arg name="bp-Malloc-BlockSize"/>
                <arg name="bp-Null-Dev"/>
                <arg name="bp-MapVirtioBlk1-Name"/>
                <arg name="bp-MapVirtioBlk1-Dev"/>
                <arg name="bp-MapVirtioBlk1-Cpumask"/>
                <arg name="bp-MapVirtioBlk1-R_Cpumask"/>
                <arg name="bp-MapVirtioBlk1-W_Cpumask"/>
                <arg name="bp-MapVirtioBlk1-FastPath"/>
                <arg name="ioengine"/>
                <arg name="runtime_sec"/>
                <arg name="rwtype"/>
            </iter>
        </test>

        <test name="fio_epilogue" type="script" auxiliary="true">
            <objective/>
            <notes/>
            <iter result="PASSED">
                <arg name="bp-Ceph-Ceph"/>
                <arg name="bp-Nvme-TransportID"/>
                <arg name="bp-Malloc-NumberOfLuns"/>
                <arg name="bp-Malloc-LunSizeInMB"/>
                <arg name="bp-Malloc-BlockSize"/>
                <arg name="bp-Null-Dev"/>
                <arg name="bp-MapVirtioBlk1-Name"/>
                <arg name="bp-MapVirtioBlk1-Dev"/>
                <arg name="bp-MapVirtioBlk1-Cpumask"/>
                <arg name="bp-MapVirtioBlk1-R_Cpumask"/>
                <arg name="bp-MapVirtioBlk1-W_Cpumask"/>
                <arg name="bp-MapVirtioBlk1-FastPath"/>
                <arg name="ioengine"/>
                <arg name="runtime_sec"/>
                <arg name="rwtype"/>
            </iter>
        </test>

        <test name="fio_perf_latency" type="script">
            <objective/>
            <iter result="PASSED">
                <arg name="bp-Ceph-Ceph"/>
                <arg name="bp-Nvme-TransportID"/>
                <arg name="bp-Malloc-NumberOfLuns"/>
                <arg name="bp-Malloc-LunSizeInMB"/>
                <arg name="bp-Malloc-BlockSize"/>
                <arg name="bp-Null-Dev"/>
                <arg name="bp-MapVirtioBlk1-Name"/>
                <arg name="bp-MapVirtioBlk1-Dev"/>
                <arg name="bp-MapVirtioBlk1-Cpumask"/>
                <arg name="bp-MapVirtioBlk1-R_Cpumask"/>
                <arg name="bp-MapVirtioBlk1-W_Cpumask"/>
                <arg name="bp-MapVirtioBlk1-FastPath"/>
                <arg name="ioengine"/>
                <arg name="runtime_sec"/>
                <arg name="rwtype"/>
                <arg name="numjobs"/>
                <arg name="iodepth"/>
                <arg name="blocksize"/>
                <arg name="rwmixread"/>
            </iter>
        </test>

        <test name="fio_perf_iops_bw" type="script">
            <objective/>
            <iter result="PASSED">
                <arg name="bp-Ceph-Ceph"/>
                <arg name="bp-Nvme-TransportID"/>
                <arg name="bp-Malloc-NumberOfLuns"/>
                <arg name="bp-Malloc-LunSizeInMB"/>
                <arg name="bp-Malloc-BlockSize"/>
                <arg name="bp-Null-Dev"/>
                <arg name="bp-MapVirtioBlk1-Name"/>
                <arg name="bp-MapVirtioBlk1-Dev"/>
                <arg name="bp-MapVirtioBlk1-Cpumask"/>
                <arg name="bp-MapVirtioBlk1-R_Cpumask"/>
                <arg name="bp-MapVirtioBlk1-W_Cpumask"/>
                <arg name="bp-MapVirtioBlk1-FastPath"/>
                <arg name="ioengine"/>
                <arg name="runtime_sec"/>
                <arg name="rwtype"/>
                <arg name="numjobs"/>
                <arg name="iodepth"/>
                <arg name="blocksize"/>
                <arg name="rwmixread"/>
            </iter>
        </test>
    </iter>
</test>
